            ]
        },
        "cwd": "/",
        "env": [
            "GOMAXPROCS=64",
            "GOTRACEBACK=crash",
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
            "TERM=xterm"
        ],
        "rlimits": [
            {
                "hard": 65536,
