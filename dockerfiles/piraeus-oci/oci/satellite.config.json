{
    "linux": {
        "maskedPaths": [
            "/proc/kcore",
            "/proc/latency_stats",
            "/proc/timer_list",
            "/proc/timer_stats",
            "/proc/sched_debug"
        ],
        "namespaces": [
            {
                "type": "mount"
            },
            {
                "type": "uts"
            }
        ],
        "readonlyPaths": [
            "/proc/asound",
            "/proc/bus",
            "/proc/fs",
            "/proc/irq",
            "/sys/firmware"
        ],
        "resources": {
            "blockIO": {},
            "cpu": {
                "shares": 0
            },
            "devices": [
                {
                    "access": "rwm",
                    "allow": true
                }
            ],
            "pids": {
                "limit": 0
            }
        },
        "rootfsPropagation": "shared"
    },
    "mounts": [
        {
            "destination": "/dev",
            "options": [
                "rbind",
                "rprivate"
            ],
            "source": "/dev",
            "type": "bind"
        },
        {
            "destination": "/etc/hosts",
            "options": [
                "rbind",
                "rprivate",
                "rw"
            ],
            "source": "/etc/hosts",
            "type": "bind"
        },
        {
            "destination": "/etc/resolv.conf",
            "options": [
                "rbind",
                "ro",
                "rprivate"
            ],
            "source": "/opt/piraeus/satellite/oci/resolv.conf",
            "type": "bind"
        },
        {
            "destination": "/lib/modules",
            "options": [
                "rbind",
                "rprivate"
            ],
            "source": "/lib/modules",
            "type": "bind"
        },
        {
            "destination": "/proc",
            "options": [
                "nosuid",
                "noexec",
                "nodev"
            ],
            "source": "proc",
            "type": "proc"
        },
        {
            "destination": "/run/docker",
            "options": [
                "rbind",
                "rprivate"
            ],
            "source": "/run/docker",
            "type": "bind"
        },
        {
            "destination": "/sys",
            "options": [
                "nosuid",
                "noexec",
                "nodev"
            ],
            "source": "sysfs",
            "type": "sysfs"
        },
        {
            "destination": "/sys/fs/cgroup",
            "options": [
                "nosuid",
                "noexec",
                "nodev"
            ],
            "source": "cgroup",
            "type": "cgroup"
        },
        {
            "destination": "/usr/src",
            "options": [
                "rbind",
                "rprivate"
            ],
            "source": "/usr/src",
            "type": "bind"
        },
        {
            "destination": "/var/run/dbus",
            "options": [
                "rbind",
                "rprivate"
            ],
            "source": "/var/run/dbus",
            "type": "bind"
        },
        {
            "destination": "/run/lvm",
            "options": [
                "",
                "rbind",
                "rprivate"
            ],
            "source": "/run/lvm",
            "type": "bind"
        },
        {
            "destination": "/var/lock",
            "options": [
                "rw",
                "rbind",
                "rprivate"
            ],
            "source": "/var/lock",
            "type": "bind"
        },
        {
            "destination": "/run/udev",
            "options": [
                "",
                "rbind",
                "rprivate"
            ],
            "source": "/run/udev",
            "type": "bind"
        },
        {
            "destination": "/var/lib/piraeus",
            "options": [
                "rw",
                "rbind"
            ],
            "source": "/var/lib/piraeus",
            "type": "bind"
        },
        {
            "destination": "/var/lib/linstor.d",
            "options": [
                "rw",
                "rbind"
            ],
            "source": "/var/lib/linstor.d",
            "type": "bind"
        },
        {
            "destination": "/etc/linstor",
            "options": [
                "rw",
                "rbind"
            ],
            "source": "/etc/linstor",
            "type": "bind"
        },
        {
            "destination": "/var/log",
            "options": [
                "rw",
                "rbind"
            ],
            "source": "/var/log",
            "type": "bind"
        }
    ],
    "ociVersion": "1.0.0",
    "process": {
        "args": [
            "/usr/share/linstor-server/bin/Satellite",
            "--logs=/var/log/linstor-satellite",
            "--config-directory=/etc/linstor",
            "--skip-hostname-check"
        ],
        "capabilities": {
